<h1>Listing tasks</h1>

<table>
  <tr>
    <th>Name</th>
    <th>project</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @tasks.each do |t| %>
  <tr>
    <td><%= t.name %></td>
    <td><%= t.project.name %></td>
    <td><%= link_to 'Show', t %></td>
    <td><%= link_to 'Edit', edit_task_path(t) %></td>
    <td><%= link_to 'Destroy', t, method: :delete  %></td>
  </tr>
<% end %>
</table>

